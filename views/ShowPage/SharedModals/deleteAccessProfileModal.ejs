<div id="delete-access-profile-modal" class="grid-modal" style="min-height: 5rem; min-width: 15rem;">
    <div id="delete-access-profile-modal-message"
        style="display: flex; justify-content: space-between; width: 100%; align-items: center;">
    </div>
    <div id="delete-access-profile-modal-memory" style="display: none;"></div>
    <div class="modal-button-container">
        <button id="delete-access-profile-modal-close" onclick="toggleDeleteAccessProfileModal(false)"
            style="color: black;">Cancel</button>
        <button id="delete-access-profile-modal-new" onclick="toggleDeleteAccessProfileModal(false, false, true)"
            style="color: red;">Delete</button>
    </div>
</div>