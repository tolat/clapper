<div id="estimate-toolbar" class="grid-toolbar">
    <div class="dropdown">
        <button class="btn btn-secondary dropdown-toggle grid-toolbar-dropdown-button" type="button"
            id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
            File
        </button>
        <ul class="dropdown-menu grid-toolbar-dropdown" aria-labelledby="dropdownMenuButton1">
            <% if(args.apOptions['Edit Estimate Versions']){%>
                <li><a class="dropdown-item" onclick="toggleEnterVersionModal(true, true, true)">New version</a></li>
                <%}%>
                    <li><a class="dropdown-item" onclick="saveData()">Save</a></li>
                    <li><a class="dropdown-item" onclick="toggleOpenVersionModal(true)">Open</a></li>
                    <% if(args.apOptions['Edit Estimate Versions']){%>
                        <li><a class="dropdown-item" onclick="toggleDeleteVersionModal(true)">Delete</a></li>
                        <li><a class="dropdown-item" onclick="toggleEnterVersionModal(true)">Rename</a>
                            <%}%>
                        </li>
        </ul>
    </div>
    <%- include('ShowPage/toolbarEditFeatures') %>
        <button class="grid-toolbar-dropdown-button" onclick="toggleFringesModal(true)">Fringes</button>
        <button class="grid-toolbar-dropdown-button" onclick="toggleManDayRatesModal(true)"
            style="margin-right: 5px; white-space: nowrap;">Man-Day Rates</button>
        <button type="button" onclick="undoCellEdit()" class="toolbar-icon"
            style="background-image: url('/public/images/undo.png'); "></button>
        <button type="button" onclick="redoCellEdit()" class="toolbar-icon"
            style="background-image: url('/public/images/redo.png'); "></button>
        <div class="grid-toolbar-item cursor-pointer active-lighten" style="white-space: nowrap;"> Comparing To Version:
            <%= comparisonVersion || 'None Set' %>
        </div>
        <div class="grid-toolbar-item" id="save-status"
            style="color: rgb(24, 135, 86); margin-left: 0px; font-weight: bold;">
            saved
        </div>
        <%- include('ShowPage/departmentsBar') %>
            <div class=" grid-toolbar-searchbar">
                Filter:
                <input id="textsearch" style="width: 150px; margin-left: 5px;">
            </div>
</div>